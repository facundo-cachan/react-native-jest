"use strict";

var _globals = require("@jest/globals");
var _reactNative = require("@testing-library/react-native");
var _customRender = require("../utils/customRender");
var _components = require("../../src/components");
var _jsxRuntime = require("react/jsx-runtime");
var text = 'Demo Text';
(0, _globals.describe)('Text component', function () {
  (0, _globals.beforeEach)(function () {
    (0, _customRender.toJSON)((0, _jsxRuntime.jsx)(_components.Text, {
      children: text
    }));
  });
  (0, _globals.test)('Render component', function () {
    (0, _globals.expect)(_reactNative.screen).toMatchSnapshot();
  });
  (0, _globals.test)('Visiblity & text value by Role', function () {
    var input = _reactNative.screen.getByText(text);
    (0, _globals.expect)(input).toBeOnTheScreen();
    (0, _globals.expect)(input).toBeVisible();
    (0, _globals.expect)(input.props.children).toBe(text);
  });
});
